// unless$(2 > 10, writeLine("Smaller"), writeLine("Bigger"))

let ifnot = fn(cond, cons, alt) {
    if !cond {
        cons()
    } else {
        alt()
    }
}

ifnot(1 > 12) #{
    writeLine("Smaller")
} #{
    writeLine("Bigger")
}