// Standard Library

// forEach array
let forEach = fn (list, func) {
    loop(fn(t) {
        func(list[t])
    }, len(list))
}

// map array
let map = fn (list, func) {
    let newList = []
    loop(fn(t) {
        let result = func(list[t])
        add(newList, result)
    }, len(list))
    return newList
}